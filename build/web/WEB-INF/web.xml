<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>addcomfeed</servlet-name>
        <servlet-class>addcomfeed</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addaudit</servlet-name>
        <servlet-class>addaudit</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>action</servlet-name>
        <servlet-class>action</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>mailer</servlet-name>
        <servlet-class>mailer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>sendmail</servlet-name>
        <servlet-class>sendmail</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>addcomfeed</servlet-name>
        <url-pattern>/addcomfeed</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addaudit</servlet-name>
        <url-pattern>/addaudit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>action</servlet-name>
        <url-pattern>/action</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>mailer</servlet-name>
        <url-pattern>/mailer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>sendmail</servlet-name>
        <url-pattern>/sendmail</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
